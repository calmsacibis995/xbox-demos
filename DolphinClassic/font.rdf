// List of resources to bundle.
//
// The output will be a header file (.h) used at compile time,
// and a packed resource file (.xpr) used at runtime.

out_header         Font.h
out_packedresource Media\Font.xpr
out_error          Font.err


Texture Font
{
   Source      Media\Fonts\Arial_16.tga
   Format      D3DFMT_A4R4G4B4
   Levels      1
}

UserData FontData
{
   DataFile Media\Fonts\Arial_16.abc
}

